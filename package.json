{
  "name": "cartographer",
  "version": "1.0.0",
  "description": "MCP server providing AI agents with a queryable architecture graph",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cartographer": "./dist/cli/index.js"
  },
  "imports": {
    "#types": "./dist/types/index.js",
    "#graph/*": "./dist/graph/*.js",
    "#anchors/*": "./dist/anchors/*.js",
    "#mcp/*": "./dist/mcp/*.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.1",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "lefthook": "^2.0.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.13"
  }
}
